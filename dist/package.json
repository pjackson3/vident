    "babel-jest": "^24.8.0",
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "eslint-watch": "^5.1.2",
    "jest": "^24.8.0",
    "jquery": "^3.4.1",
    "mustache": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "page": "^1.11.4",
    "rollup": "^1.15.1",
    "rollup-plugin-babel": "^4.3.2"
  },
  "peerDependencies": {
    "jquery": "^3.4.1",
    "mustache": "^3.0.1",
    "page": "^1.11.4"
